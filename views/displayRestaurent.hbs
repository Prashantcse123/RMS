                    </th>
                    <th>
                        comment
                    </th>
                    <th>
                        Name:
                    </th>
                    <th>
                        Email:
                    </th>
                </tr>
                {{#each data.comment}}
                <tr>
                    <td>
                        <img src={{this.userID.profilePicPath}} height="50" width="50" />
                    </td>
                    <td>{{this.commentText}}</td>
                    <td>{{this.userID.name}}</td>
                    <td>{{this.userID.email}}</td>
                </tr>
                {{/each}}
            </table>